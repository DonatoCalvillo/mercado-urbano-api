{
  "version": 1,
  "env": {
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port",
    "DB_NAME": "@bd_name",
    "DB_USERNAME": "@db_name",
    "DB_PASSWORD": "@db_password",
    "PORT": "@port",
    "JWT_SECRET": "@jwt_secret"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
